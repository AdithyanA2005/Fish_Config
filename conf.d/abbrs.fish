# GIT ABBREVATIONS
abbr gst 'git status'
abbr gla 'git log --graph --oneline --all'
abbr gaa 'git add .'
abbr gco 'git checkout'
abbr gp 'git push'
abbr gpom 'git push origin main'
abbr gra 'git remote add'
abbr graom 'git remote add origin main'

# PYTHON ABBREVATIONS
abbr py 'python3'
abbr pyenv 'source venv/bin/activate.fish'
abbr vpy './venv/bin/python3'
abbr vpip './venv/bin/pip'

# NODE ABBREVATIONS
abbr ya 'yarn add'
abbr yr 'yarn remove'

# PACKAGE MANAGER ABBREVATIONS
abbr sai 'sudo apt install'
abbr sagi 'sudo apt-get install'

# MONGODB ABBREVATIONS
abbr mongodstart 'sudo service mongod start'
abbr mongodstop 'sudo service mongod stop'
abbr mongodstatus 'sudo service mongod status'
